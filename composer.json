{
    "name": "t73biz/cakephp-turntable",
    "description": "Turntable is a Foundation 6 solution for CakePHP 3.x using.",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Turntable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Turntable\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
